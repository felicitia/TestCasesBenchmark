<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <TextView android:id="@+id/btn_expand" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/StructuredPoliciesButtonSeeMore"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/expansion_section" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/txt_subhead_processing_time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/structured_shipping_processing_time" style="?attr/structuredPoliciesSubheadStyle"/>
        <TextView android:id="@+id/txt_processing_time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fixed_medium" style="?attr/structuredPoliciesTextStyle"/>
        <Space android:id="@+id/spacer_1" android:layout_width="match_parent" android:layout_height="@dimen/fixed_xlarge"/>
        <TextView android:id="@+id/txt_subhead_shipping_time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/structured_shipping_estimated_time" style="?attr/structuredPoliciesSubheadStyle"/>
        <TextView android:id="@+id/txt_shipping_time" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fixed_medium" android:lineSpacingExtra="@dimen/fixed_medium" style="?attr/structuredPoliciesTextStyle"/>
        <TextView android:id="@+id/txt_shipping_disclaimer" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fixed_large" android:text="@string/structured_shipping_message" style="?attr/structuredPoliciesTextLightStyle"/>
        <Space android:id="@+id/spacer_2" android:layout_width="match_parent" android:layout_height="@dimen/fixed_xlarge"/>
        <TextView android:id="@+id/txt_subhead_customs_fees" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/structured_shipping_customs_duties_fees" style="?attr/structuredPoliciesSubheadStyle"/>
        <TextView android:id="@+id/txt_customs_fees" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fixed_medium" android:text="@string/structured_shipping_customs_duties_fees_info" style="?attr/structuredPoliciesTextStyle"/>
    </LinearLayout>
</merge>
