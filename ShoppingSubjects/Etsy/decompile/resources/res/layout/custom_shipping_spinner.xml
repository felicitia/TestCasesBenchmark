<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <android.support.v7.widget.AppCompatSpinner android:id="@+id/shipping_spinner" android:layout_width="match_parent" android:layout_height="wrap_content" style="@style/Etsy.Widget.Spinner"/>
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <android.support.v7.widget.AppCompatSpinner android:id="@+id/min_spinner" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" style="@style/Etsy.Widget.Spinner"/>
        <TextView android:paddingLeft="@dimen/margin_large" android:paddingRight="@dimen/margin_large" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-5dp" android:text="@string/to" style="@style/TextBlack"/>
        <android.support.v7.widget.AppCompatSpinner android:id="@+id/max_spinner" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" style="@style/Etsy.Widget.Spinner"/>
    </LinearLayout>
    <com.etsy.android.uikit.view.SwitchToggle android:id="@+id/unit_toggle" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/fixed_medium" android:layout_marginBottom="@dimen/fixed_medium" android:textOn="@string/structured_shipping_business_days" android:textOff="@string/structured_shipping_weeks"/>
</merge>
