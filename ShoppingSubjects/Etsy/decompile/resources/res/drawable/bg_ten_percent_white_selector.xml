<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:enterFadeDuration="@integer/touch_enter_fade_speed" android:exitFadeDuration="@integer/touch_exit_fade_speed">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/ten_percent_white"/>
        </shape>
    </item>
    <item android:state_focused="true" android:drawable="@color/item_focused">
        <shape android:shape="rectangle">
            <solid android:color="@color/ten_percent_white"/>
        </shape>
    </item>
    <item android:state_checked="true" android:drawable="@color/item_focused">
        <shape android:shape="rectangle">
            <solid android:color="@color/ten_percent_white"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</selector>
