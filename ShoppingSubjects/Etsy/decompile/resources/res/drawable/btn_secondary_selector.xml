<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <solid android:color="@color/sk_gray_20"/>
            <stroke android:width="1dp" android:color="@color/sk_gray_30"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <solid android:color="@color/sk_gray_20"/>
            <stroke android:width="1dp" android:color="@color/sk_gray_40"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <stroke android:width="1dp" android:color="@color/sk_gray_30"/>
        </shape>
    </item>
</selector>
