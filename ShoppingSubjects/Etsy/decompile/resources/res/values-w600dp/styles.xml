<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Avatar.Convos.New.Conversation">
    </style>
    <style name="Avatar.Convos.SOE.List">
    </style>
    <style name="ButtonWhiteV2">
        <item name="android:background">@drawable/btn_white_selector_v2</item>
        <item name="android:paddingLeft">@dimen/padding_medium</item>
        <item name="android:paddingRight">@dimen/padding_medium</item>
    </style>
    <style name="CartListingPanelDropDownSpinner">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:spinnerMode">@null</item>
    </style>
    <style name="CartListingPanelOptionsTable">
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="CartNote">
        <item name="android:textAppearance">@style/TextGrey</item>
        <item name="android:background">@drawable/edit_text_material</item>
    </style>
    <style name="ConvoCard">
        <item name="android:background">@drawable/bg_card</item>
        <item name="android:padding">@dimen/padding_medium_large</item>
    </style>
    <style name="ConvoCard.ClickState">
        <item name="android:background">@drawable/bg_card_selector</item>
    </style>
    <style name="ConvoThread">
        <item name="android:paddingLeft">@dimen/convo_card_padding</item>
        <item name="android:paddingTop">@dimen/convo_card_padding</item>
        <item name="android:paddingRight">@dimen/convo_card_padding</item>
        <item name="android:paddingBottom">@dimen/convo_card_padding</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:divider">@color/transparent</item>
        <item name="android:dividerHeight">@dimen/padding_medium</item>
    </style>
    <style name="DropDownButton">
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:background">@drawable/spinner_material</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="EtsyToolbarStyle">
        <item name="android:background">@color/sk_bg_white</item>
        <item name="android:paddingRight">16dp</item>
        <item name="titleTextAppearance">@style/EtsyToolbarTextStyle</item>
    </style>
    <style name="MultiShopCartGroup">
        <item name="android:background">@color/cardview_light_background</item>
        <item name="android:layout_marginLeft">@dimen/msco_cart_group_spacing</item>
        <item name="android:layout_marginRight">@dimen/msco_cart_group_spacing</item>
        <item name="android:layout_marginBottom">@dimen/msco_cart_group_spacing</item>
    </style>
    <style name="MultiShopCartItem">
        <item name="android:paddingLeft">@dimen/padding_medium</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingRight">@dimen/padding_medium</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:layout_marginLeft">@dimen/msco_cart_group_spacing</item>
        <item name="android:layout_marginRight">@dimen/msco_cart_group_spacing</item>
    </style>
    <style name="ReceiptSubTitleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/margin_medium_large</item>
        <item name="android:minWidth">@dimen/receipt_title_width</item>
    </style>
    <style name="ReceiptTitleText">
        <item name="android:layout_width">@dimen/receipt_title_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/margin_medium_large</item>
    </style>
    <style name="ReceiptTotalsDivider">
        <item name="android:background">@color/divider_header</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_height</item>
        <item name="android:layout_marginTop">@dimen/margin_medium_large</item>
    </style>
    <style name="SavedForLaterItem">
        <item name="android:background">@color/cardview_light_background</item>
        <item name="android:paddingTop">@dimen/padding_small</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:layout_marginLeft">@dimen/margin_medium</item>
        <item name="android:layout_marginRight">@dimen/margin_medium</item>
    </style>
    <style name="Theme.Etsy.DialogActivity">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
</resources>
