<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/branded_feed_view_banner" android:layout_width="match_parent" android:layout_height="@dimen/branded_banner_height">
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center" android:layout_width="match_parent" android:layout_height="match_parent" app:drawable="@drawable/wish_outlet_banner"/>
        <LinearLayout android:layout_gravity="center" android:orientation="vertical" android:paddingLeft="@dimen/sixteen_padding" android:paddingRight="@dimen/sixteen_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_title" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="center" android:layout_gravity="center" android:id="@+id/branded_feed_view_banner_text_old" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/sixteen_padding" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="center" android:layout_gravity="center" android:id="@+id/branded_feed_view_banner_text" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/twenty_four_padding" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="center" android:layout_gravity="center" android:id="@+id/branded_feed_view_banner_text_subtitle" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/branded_feed_view_banner_x_button" android:layout_width="@dimen/double_screen_padding" android:layout_height="@dimen/double_screen_padding" android:layout_marginTop="@dimen/screen_padding" android:layout_marginRight="@dimen/screen_padding" app:drawable="@drawable/textview_clear"/>
        <View android:layout_gravity="bottom" android:background="@drawable/light_shadow" android:layout_width="match_parent" android:layout_height="@dimen/half_screen_padding"/>
    </FrameLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/branded_feed_view_category_view" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sixteen_padding">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/screen_padding">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/text_primary" android:layout_gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/browse_by_category" android:layout_weight="1"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/text_secondary" android:layout_gravity="center_vertical" android:id="@+id/branded_feed_view_category_view_all" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/screen_padding" android:text="@string/view_all"/>
        </LinearLayout>
        <com.contextlogic.wish.ui.listview.HorizontalListView android:id="@+id/branded_feed_view_category_list_view" android:layout_width="match_parent" android:layout_height="@dimen/branded_category_list_height" android:layout_marginTop="@dimen/sixteen_padding"/>
    </LinearLayout>
    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/text_primary" android:layout_gravity="left" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/screen_padding" android:layout_marginTop="@dimen/sixteen_padding" android:text="@string/latest_brand"/>
</LinearLayout>
