<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <FrameLayout android:id="@+id/loading_page_content_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <com.contextlogic.wish.ui.loading.SafeSwipeRefreshLayout android:id="@+id/loading_page_refresh_content_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/loading_page_no_items_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center" android:id="@+id/loading_page_no_items_image_view" android:layout_width="@dimen/notifications_fragment_empty_notifications_width" android:layout_height="@dimen/notifications_fragment_empty_notifications_height"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/gray2" android:gravity="center" android:id="@+id/loading_page_no_items_caption_text" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sixteen_padding" android:layout_marginBottom="@dimen/four_padding"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/text_secondary" android:gravity="center" android:layout_gravity="center_horizontal" android:id="@+id/loading_page_no_items_message_text" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/twenty_four_padding"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_button" android:textStyle="bold" android:textColor="@android:color/white" android:gravity="center" android:layout_gravity="center_horizontal" android:id="@+id/loading_page_no_items_view_browse_button" android:background="@drawable/primary_button_selector" android:visibility="gone" android:layout_width="@dimen/empty_cart_button_width" android:layout_height="@dimen/default_button_height" android:text="@string/continue_shopping"/>
    </LinearLayout>
    <FrameLayout android:layout_gravity="top" android:id="@+id/loading_page_loading_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.contextlogic.wish.ui.loading.PrimaryProgressBar android:layout_gravity="center" android:id="@+id/loading_page_primary_progress_bar" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/Widget.Wish.ProgressBar.Medium"/>
        <com.contextlogic.wish.ui.loading.ThreeDotProgressBar android:layout_gravity="center" android:id="@+id/loading_page_three_dot_progress_bar" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </FrameLayout>
    <FrameLayout android:layout_gravity="top" android:id="@+id/loading_page_error_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:gravity="center" android:layout_gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/text_secondary" android:gravity="center" android:layout_gravity="center" android:id="@+id/loading_page_error_message_text" android:paddingLeft="@dimen/double_screen_padding" android:paddingRight="@dimen/double_screen_padding" android:paddingBottom="@dimen/double_screen_padding" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/loading_error" android:singleLine="false"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_button" android:textStyle="bold" android:textColor="@color/text_selector_main_primary" android:gravity="center" android:layout_gravity="center" android:id="@+id/loading_page_try_again_button" android:layout_width="wrap_content" android:layout_height="@dimen/default_button_height" android:text="@string/try_again" android:textAllCaps="true"/>
        </LinearLayout>
    </FrameLayout>
    <LinearLayout android:gravity="center" android:layout_gravity="top" android:orientation="vertical" android:id="@+id/no_internet_connection_error_view" android:background="@color/gray6" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerHorizontal="true" android:layout_centerVertical="true">
        <FrameLayout android:background="@drawable/white_circle_gray_outline" android:layout_width="@dimen/no_internet_connection_icon_size" android:layout_height="@dimen/no_internet_connection_icon_size">
            <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center" android:layout_width="@dimen/internet_connection_icon_image_width" android:layout_height="@dimen/internet_connection_icon_image_height" android:src="@drawable/no_internet_icon"/>
        </FrameLayout>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_fourteen" android:textStyle="bold" android:textColor="@color/gray2" android:paddingTop="@dimen/sixteen_padding" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/no_internet_connection"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textColor="@color/gray2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/four_padding" android:text="@string/check_your_connection_and_try_again"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textStyle="bold" android:textColor="@color/white" android:gravity="center" android:id="@+id/no_internet_connection_try_again_button" android:background="@drawable/main_button_selector" android:paddingLeft="@dimen/fourty_eight_padding" android:paddingRight="@dimen/fourty_eight_padding" android:layout_width="wrap_content" android:layout_height="@dimen/no_internet_try_again_button_height" android:layout_marginTop="@dimen/twenty_padding" android:text="@string/try_again" android:textAllCaps="false"/>
    </LinearLayout>
</merge>
