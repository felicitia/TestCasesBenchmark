    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/cool_gray1" android:gravity="left" android:id="@+id/product_details_fragment_overview_return_policy_text" android:layout_width="match_parent" android:layout_height="wrap_content"/>
</LinearLayout>
