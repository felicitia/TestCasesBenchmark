<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:id="@+id/product_details_shipping_row_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:divider="@drawable/transparent_sixteen_divider" android:showDividers="middle"/>
    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/gray1" android:gravity="left" android:id="@+id/product_details_fragment_overview_shipping_merchant" android:paddingTop="@dimen/sixteen_padding" android:paddingBottom="@dimen/sixteen_padding" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <RelativeLayout android:gravity="left|center_vertical|center_horizontal|center" android:id="@+id/collapsible_shipping_return_policy_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/sixteen_padding">
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textStyle="bold" android:textColor="@color/cool_gray1" android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/return_policy" android:layout_toLeftOf="@+id/product_details_fragment_overview_shipping_return_policy_details" android:layout_alignParentLeft="true"/>
