<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <com.contextlogic.wish.ui.image.AutoReleasableImageView android:background="@drawable/signup_flow_category_border_gray" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <com.contextlogic.wish.ui.image.AutoReleasableImageView android:id="@+id/signup_category_border" android:background="@drawable/signup_flow_category_border_blue" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <RelativeLayout android:paddingTop="@dimen/two_padding" android:paddingRight="@dimen/two_padding" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_small" android:textColor="@color/cool_gray3" android:ellipsize="end" android:gravity="center" android:id="@+id/signup_category_name" android:paddingBottom="@dimen/signup_flow_select_category_text_margin" android:layout_width="match_parent" android:layout_height="wrap_content" android:maxLines="2" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
