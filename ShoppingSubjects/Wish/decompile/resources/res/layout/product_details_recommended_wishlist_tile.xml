<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.contextlogic.wish.ui.image.NetworkImageView android:layout_gravity="center" android:id="@+id/tile_left_main_image" android:layout_width="@dimen/discover_wishlist_large_tile_width" android:layout_height="@dimen/discover_wishlist_large_tile_height"/>
        <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/four_padding">
            <com.contextlogic.wish.ui.image.NetworkImageView android:layout_gravity="center" android:id="@+id/tile_right_top_image" android:layout_width="@dimen/discover_wishlist_small_tile_height" android:layout_height="@dimen/discover_wishlist_small_tile_height" android:layout_marginBottom="@dimen/four_padding" android:scaleType="centerCrop"/>
            <com.contextlogic.wish.ui.image.NetworkImageView android:layout_gravity="center" android:id="@+id/tile_right_bottom_image" android:layout_width="@dimen/discover_wishlist_small_tile_height" android:layout_height="@dimen/discover_wishlist_small_tile_height" android:scaleType="centerCrop"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.contextlogic.wish.ui.view.ProfileImageView android:layout_gravity="top" android:id="@+id/tile_user_profile_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/twelve_padding" app:imageSize="@dimen/discover_wishlist_profile_picture_tile_width" app:profileTextSize="@dimen/text_size_fourteen"/>
        <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/eight_padding" android:layout_marginTop="@dimen/ten_padding">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_fourteen" android:textStyle="bold" android:textColor="@color/text_primary" android:ellipsize="end" android:id="@+id/tile_wishlist_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_fourteen" android:textColor="@color/cool_gray3" android:ellipsize="end" android:id="@+id/tile_wishlist_creator_username" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
