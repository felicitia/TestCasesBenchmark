<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center" android:layout_gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/onfido_fs_normal" android:textColor="@color/onfido_dark_300" android:gravity="center" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/onfido_activity_horizontal_margin" android:layout_marginTop="4dp" android:layout_marginRight="@dimen/onfido_activity_horizontal_margin" android:text="@string/onfido_country_selection_empty_state_title" style="@style/OnfidoTextStyle"/>
        <TextView android:textSize="@dimen/onfido_fs_small" android:textColor="@color/onfido_dark_300" android:gravity="center" android:id="@+id/message" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/onfido_activity_horizontal_margin" android:layout_marginTop="@dimen/onfido_country_selection_message_top_margin" android:layout_marginRight="@dimen/onfido_activity_horizontal_margin" android:text="@string/onfido_country_selection_empty_state_message" style="@style/OnfidoTextStyle"/>
        <Button android:id="@+id/selectAnotherCountry" android:layout_width="wrap_content" android:layout_height="@dimen/onfido_country_selection_action_height" android:layout_marginTop="@dimen/onfido_country_selection_action_top_margin" android:layout_marginBottom="4dp" android:text="@string/onfido_select_another_document" style="@style/OnfidoMessageButtonStyle"/>
    </LinearLayout>
</FrameLayout>
