<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingLeft="@dimen/sixteen_padding" android:paddingRight="@dimen/sixteen_padding" android:descendantFocusability="blocksDescendants" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/screen_padding" android:layout_marginBottom="@dimen/four_padding">
        <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:orientation="horizontal" android:id="@+id/fragment_wishlist_profile_item_title_container" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textColor="@color/gray1" android:ellipsize="end" android:id="@+id/fragment_wishlist_profile_item_title" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true"/>
                <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center_vertical" android:id="@+id/fragment_wishlist_private_icon" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="@dimen/four_padding" android:adjustViewBounds="true" android:layout_marginStart="@dimen/four_padding" app:drawable="@drawable/wishlist_lock_gray"/>
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
                <com.contextlogic.wish.ui.text.ThemedTextView android:textColor="@color/gray3" android:id="@+id/fragment_wishlist_profile_item_view_count" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            </LinearLayout>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_caption" android:textColor="@color/gray3" android:gravity="top" android:layout_gravity="start" android:id="@+id/fragment_wishlist_profile_item_sub_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/fragment_wishlist_profile_item_title_container"/>
        </LinearLayout>
        <ImageButton android:gravity="end" android:orientation="horizontal" android:id="@+id/fragment_wishlist_menu_button" android:background="@color/transparent" android:paddingLeft="@dimen/eight_padding" android:layout_width="wrap_content" android:layout_height="@dimen/min_button_height" android:src="@drawable/profile_more_icon" android:scaleType="center" android:paddingStart="@dimen/eight_padding"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:id="@+id/fragment_wishlist_profile_item_images_row1" android:layout_width="match_parent" android:layout_height="@dimen/profile_fragment_row_image_section_height"/>
    <LinearLayout android:orientation="horizontal" android:id="@+id/fragment_wishlist_profile_item_images_row2" android:layout_width="match_parent" android:layout_height="@dimen/profile_fragment_row_image_section_height" android:layout_marginTop="@dimen/half_screen_padding" android:layout_marginBottom="@dimen/double_screen_padding"/>
</LinearLayout>
