<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <ScrollView android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/signup_free_gift_shipping_view_floating_bottom_padding">
            <LinearLayout android:orientation="vertical" android:padding="@dimen/screen_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
                <LinearLayout android:gravity="center" android:layout_gravity="left" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/double_screen_padding">
                    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="left|center_vertical|center_horizontal|center" android:layout_gravity="left|center_vertical|center_horizontal|center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/ship_to_colon" android:singleLine="false"/>
                    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_caption" android:textColor="@color/text_secondary" android:gravity="right|center_vertical|center_horizontal|center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/signup_free_gift_shipping_view_cancel" android:layout_width="@dimen/zero_padding" android:layout_height="wrap_content" android:text="@string/no_thanks" android:layout_weight="1"/>
                </LinearLayout>
                <LinearLayout android:orientation="vertical" android:id="@+id/signup_free_gift_shipping_view_signup_flow_layout" android:layout_width="match_parent" android:layout_height="match_parent">
                    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="center_horizontal" android:layout_gravity="center_horizontal" android:id="@+id/signup_free_gift_shipping_view_where_to_ship_textview" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1"/>
                    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content">
                        <com.contextlogic.wish.ui.image.NetworkImageView android:layout_gravity="top" android:id="@+id/signup_free_gift_shipping_view_product_image" android:layout_width="@dimen/free_gift_fragment_shipping_view_product_image_size" android:layout_height="@dimen/free_gift_fragment_shipping_view_product_image_size" android:layout_margin="@dimen/screen_padding" android:layout_centerInParent="true"/>
                    </RelativeLayout>
                    <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/signup_free_gift_shipping_view_price_layout" android:layout_width="match_parent" android:layout_height="29dp">
                        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/text_primary" android:layout_gravity="center" android:id="@+id/signup_free_gift_shipping_view_price_sub_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/screen_padding"/>
                        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_subtitle" android:textStyle="bold" android:textColor="@color/text_primary" android:layout_gravity="center" android:id="@+id/signup_free_gift_shipping_view_price_main_text" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:layout_gravity="left" android:orientation="horizontal" android:id="@+id/signup_free_gift_shipping_view_use_billing_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/screen_padding">
                    <CheckBox android:layout_gravity="center" android:id="@+id/signup_free_gift_shipping_view_use_billing_checkbox" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/eight_padding" android:layout_marginEnd="@dimen/eight_padding"/>
                    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textColor="@color/text_primary" android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/same_as_billing_address" android:singleLine="false"/>
                </LinearLayout>
                <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_caption" android:textColor="@color/text_secondary" android:gravity="left" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/half_screen_padding" android:text="@string/asterisk_indicates_a_field_is_required"/>
                <com.contextlogic.wish.activity.cart.shipping.ShippingAddressFormView android:id="@+id/signup_free_gift_shipping_view_shipping_form_view" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_button" android:textStyle="bold" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/signup_free_gift_shipping_view_inline_done_button" android:background="@drawable/orange_button_selector" android:layout_width="match_parent" android:layout_height="@dimen/default_button_height" android:layout_marginTop="@dimen/screen_padding"/>
            </LinearLayout>
        </ScrollView>
        <LinearLayout android:layout_gravity="bottom" android:orientation="vertical" android:id="@+id/signup_free_gift_shipping_view_floating_done_button_container" android:background="@drawable/white_background" android:padding="@dimen/screen_padding" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_button" android:textStyle="bold" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/signup_free_gift_shipping_view_floating_done_button" android:background="@drawable/orange_button_selector" android:layout_width="match_parent" android:layout_height="@dimen/default_button_height"/>
        </LinearLayout>
    </FrameLayout>
</merge>
