<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <RelativeLayout android:layout_gravity="center" android:background="@drawable/white_rounded_background" android:layout_width="@dimen/deal_dash_educational_modal_width" android:layout_height="@dimen/deal_dash_educational_modal_height">
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:id="@+id/deal_dash_educational_modal_cancel_button" android:padding="@dimen/screen_padding" android:layout_width="@dimen/deal_dash_educational_modal_cancel_size" android:layout_height="@dimen/deal_dash_educational_modal_cancel_size" android:layout_marginTop="@dimen/half_screen_padding" android:layout_marginRight="@dimen/half_screen_padding" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:layout_marginEnd="@dimen/half_screen_padding" android:layout_alignParentEnd="true" app:drawable="@drawable/textview_clear"/>
        <LinearLayout android:orientation="vertical" android:id="@+id/deal_dash_educational_modal_1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/deal_dash_educational_modal_cancel_size" android:layout_alignParentTop="true" android:layout_centerHorizontal="true">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textStyle="normal|bold|italic" android:textColor="@color/text_primary" android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/double_screen_padding" android:layout_marginRight="@dimen/double_screen_padding" android:text="@string/blitz_buy_educational_1_1"/>
            <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center" android:layout_width="@dimen/deal_dash_educational_modal_discount_width" android:layout_height="@dimen/deal_dash_educational_modal_discount_height" android:layout_marginTop="@dimen/double_screen_padding" android:layout_marginBottom="@dimen/double_screen_padding" app:drawable="@drawable/dealdash_discount"/>
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textStyle="normal|bold|italic" android:textColor="@color/text_primary" android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/double_screen_padding" android:layout_marginRight="@dimen/double_screen_padding" android:text="@string/blitz_buy_educational_1_2"/>
            <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_gravity="center" android:layout_width="@dimen/deal_dash_educational_modal_calendar_size" android:layout_height="@dimen/deal_dash_educational_modal_calendar_size" android:layout_marginTop="@dimen/double_screen_padding" app:drawable="@drawable/dealdash_calendar"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/deal_dash_educational_modal_2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/deal_dash_educational_modal_cancel_size" android:layout_alignParentTop="true" android:layout_centerHorizontal="true">
            <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textStyle="normal|bold|italic" android:textColor="@color/text_primary" android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/screen_padding" android:layout_marginRight="@dimen/screen_padding" android:text="@string/blitz_buy_educational_2_product"/>
            <RelativeLayout android:layout_gravity="center_horizontal" android:layout_width="@dimen/deal_dash_educational_modal_spinner_size" android:layout_height="@dimen/deal_dash_educational_modal_spinner_size" android:layout_marginTop="@dimen/double_screen_padding">
                <com.contextlogic.wish.ui.image.AutoReleasableImageView android:id="@+id/deal_dash_educational_modal_spinner" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerInParent="true" app:drawable="@drawable/dealdash_spinner_key"/>
                <RelativeLayout android:id="@+id/deal_dash_spin_button_container" android:layout_width="@dimen/deal_dash_educational_modal_spinner_button_size" android:layout_height="@dimen/deal_dash_educational_modal_spinner_button_size" android:layout_centerInParent="true">
                    <com.contextlogic.wish.ui.image.AutoReleasableImageView android:layout_width="@dimen/deal_dash_educational_modal_spinner_button_size" android:layout_height="@dimen/deal_dash_educational_modal_spinner_button_size" android:layout_marginBottom="@dimen/screen_padding" android:layout_centerInParent="true" app:drawable="@drawable/dealdash_spin_button"/>
                    <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_caption" android:textStyle="normal|bold|italic" android:textColor="@color/dark_gray_1" android:gravity="center" android:id="@+id/deal_dash_spin_text_view" android:layout_width="@dimen/deal_dash_educational_modal_spinner_button_size" android:layout_height="@dimen/triple_screen_padding" android:text="@string/blitz_buy_spin" android:layout_centerInParent="true" android:resizeable="true"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:id="@+id/deal_dash_educational_modal_left_button" android:layout_width="wrap_content" android:layout_height="@dimen/triple_screen_padding" android:layout_marginLeft="@dimen/screen_padding" android:layout_marginBottom="@dimen/screen_padding" android:layout_alignParentLeft="true" android:layout_alignParentBottom="true" android:layout_marginStart="@dimen/screen_padding" android:layout_alignParentStart="true" app:drawable="@drawable/dealdash_left_selector"/>
        <com.contextlogic.wish.ui.text.ThemedTextView android:textSize="@dimen/text_size_body" android:textStyle="bold" android:textColor="@color/text_primary" android:gravity="center" android:id="@+id/deal_dash_educational_modal_page_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/one_half_screen_padding" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        <com.contextlogic.wish.ui.image.AutoReleasableImageView android:id="@+id/deal_dash_educational_modal_right_button" android:layout_width="wrap_content" android:layout_height="@dimen/triple_screen_padding" android:layout_marginRight="@dimen/screen_padding" android:layout_marginBottom="@dimen/screen_padding" android:layout_alignParentRight="true" android:layout_alignParentBottom="true" android:layout_marginEnd="@dimen/screen_padding" android:layout_alignParentEnd="true" app:drawable="@drawable/dealdash_right_selector"/>
    </RelativeLayout>
</FrameLayout>
