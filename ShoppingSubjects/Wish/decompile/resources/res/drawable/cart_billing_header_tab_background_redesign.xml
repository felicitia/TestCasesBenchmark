<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/two_padding" android:color="@color/main_primary"/>
            <solid android:color="@color/main_primary_10"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/divider" android:color="@color/gray5"/>
            <solid android:color="@color/gray7"/>
        </shape>
    </item>
</selector>
