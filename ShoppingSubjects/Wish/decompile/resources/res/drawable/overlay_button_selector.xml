<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="false" android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/overlay_button"/>
            <stroke android:width="1px" android:color="@color/overlay_button_border"/>
            <corners android:radius="@dimen/product_feed_button_bg_selector_corner_radius"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/overlay_button_pressed"/>
            <stroke android:width="1px" android:color="@color/overlay_button_border_pressed"/>
            <corners android:radius="@dimen/product_feed_button_bg_selector_corner_radius"/>
        </shape>
    </item>
    <item android:state_focused="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/overlay_button_pressed"/>
            <stroke android:width="1px" android:color="@color/overlay_button_border_pressed"/>
            <corners android:radius="@dimen/product_feed_button_bg_selector_corner_radius"/>
        </shape>
    </item>
</selector>
