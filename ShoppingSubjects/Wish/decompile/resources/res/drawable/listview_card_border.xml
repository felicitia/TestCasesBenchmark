<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/gray5"/>
            <corners android:radius="@dimen/small_corner_radius"/>
        </shape>
    </item>
    <item android:left="@dimen/default_border_stroke" android:top="@dimen/default_border_stroke" android:right="@dimen/default_border_stroke" android:bottom="@dimen/default_border_stroke">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/small_corner_radius"/>
        </shape>
    </item>
</layer-list>
