<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true" android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_color_selector_dialog_fragment_text_swatch_corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/size_color_selector_dialog_fragment_swatch_outline_thickness" android:color="@color/wish_blue"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_color_selector_dialog_fragment_text_swatch_corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/size_color_selector_dialog_fragment_swatch_outline_thickness" android:color="@color/gray5"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_color_selector_dialog_fragment_text_swatch_corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/size_color_selector_dialog_fragment_swatch_outline_thickness" android:color="@color/gray5"/>
        </shape>
    </item>
</selector>
